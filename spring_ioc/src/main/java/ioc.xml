<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="e1"
		class="in.kenz.springioc.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Jalib"></property>
	</bean>
	<bean id="e2"
		class="in.kenz.springioc.Employee">
		<property name="id" value="2"></property>
		<property name="name" value="Kenz"></property>
	</bean>
	
	
	<bean id="s1"
		class="in.kenz.springioc.Student">
		<constructor-arg name="id" value="1"></constructor-arg>
		<constructor-arg name="name" value="Jalib"></constructor-arg>
	</bean>
	<bean id="s2"
		class="in.kenz.springioc.Student">
		<constructor-arg name="id" value="2"></constructor-arg>
		<constructor-arg name="name" value="Kenz"></constructor-arg>
	</bean>


</beans>


